# Environment Configuration for Docker Compose
# Copy this file to .env and fill in your values

# LLM API Keys (at least one is required for task extraction)
GROQ_API_KEY=your_groq_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GOOGLE_API_KEY=your_google_api_key_here

# Database Configuration (defaults are fine for local Docker)
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=taskflow

# RabbitMQ Configuration (defaults are fine for local Docker)
RABBITMQ_DEFAULT_USER=guest
RABBITMQ_DEFAULT_PASS=guest

# Application Configuration
LOG_LEVEL=INFO
